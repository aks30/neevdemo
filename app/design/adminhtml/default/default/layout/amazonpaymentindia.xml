<layout version="0.1.0">
    <default>
        <reference name="head">
            <action method="addCss"><stylesheet>neev/css/amazonpaymentindia.css</stylesheet></action>
        </reference>
        <reference name="before_body_end">
            <block type="core/template" template="neev/amazonpaymentindia/init.phtml" />
        </reference>
    </default>
    <amazonpaymentindia_adminhtml_debug_index>
        <reference name="content">
            <block type="amazonpaymentindia/adminhtml_debug" name="amazonpaymentindia_debug_info" />
        </reference>
    </amazonpaymentindia_adminhtml_debug_index>
    <amazonpaymentindia_adminhtml_logger_exceptions_index>
        <reference name="content">
            <block type="amazonpaymentindia/adminhtml_logger_exceptions" name="amazon_exceptions" />
        </reference>
    </amazonpaymentindia_adminhtml_logger_exceptions_index>
    <amazonpaymentindia_adminhtml_logger_exceptions_view>
        <reference name="head">
            <action method="addJs"><script>neev/prettify.js</script></action>
            <action method="addJs"><script>neev/modalbox.js</script></action>
            <action method="addCss"><stylesheet>neev/css/modalbox.css</stylesheet></action>
        </reference>
    </amazonpaymentindia_adminhtml_logger_exceptions_view>
    <amazonpaymentindia_adminhtml_logger_feeds_index>
        <reference name="content">
            <block type="amazonpaymentindia/adminhtml_logger_feeds" name="amazon_feeds" />
        </reference>
    </amazonpaymentindia_adminhtml_logger_feeds_index>
    <amazonpaymentindia_adminhtml_logger_feeds_view>
        <reference name="head">
            <action method="addJs"><script>neev/prettify.js</script></action>
            <action method="addJs"><script>neev/modalbox.js</script></action>
            <action method="addCss"><stylesheet>neev/css/modalbox.css</stylesheet></action>
        </reference>
    </amazonpaymentindia_adminhtml_logger_feeds_view>
    <amazonpaymentindia_adminhtml_logger_notifications_index>
        <reference name="content">
            <block type="amazonpaymentindia/adminhtml_logger_notifications" name="amazon_notifications" />
        </reference>
    </amazonpaymentindia_adminhtml_logger_notifications_index>
    <amazonpaymentindia_adminhtml_logger_notifications_view>
        <reference name="head">
            <action method="addJs"><script>neev/prettify.js</script></action>
            <action method="addJs"><script>neev/modalbox.js</script></action>
            <action method="addCss"><stylesheet>neev/css/modalbox.css</stylesheet></action>
        </reference>
    </amazonpaymentindia_adminhtml_logger_notifications_view>
    <amazonpaymentindia_adminhtml_logger_reports_index>
        <reference name="content">
            <block type="amazonpaymentindia/adminhtml_logger_reports" name="amazon_reports" />
        </reference>
    </amazonpaymentindia_adminhtml_logger_reports_index>
    <amazonpaymentindia_adminhtml_logger_reports_view>
        <reference name="head">
            <action method="addJs"><script>neev/prettify.js</script></action>
            <action method="addJs"><script>neev/modalbox.js</script></action>
            <action method="addCss"><stylesheet>neev/css/modalbox.css</stylesheet></action>
        </reference>
    </amazonpaymentindia_adminhtml_logger_reports_view>
    <amazonpaymentindia_adminhtml_logger_api_index>
        <reference name="content">
            <block type="amazonpaymentindia/adminhtml_logger_api" name="amazon_api" />
        </reference>
    </amazonpaymentindia_adminhtml_logger_api_index>
    <amazonpaymentindia_adminhtml_logger_api_view>
        <reference name="head">
            <action method="addJs"><script>neev/prettify.js</script></action>
            <action method="addJs"><script>neev/modalbox.js</script></action>
            <action method="addCss"><stylesheet>neev/css/modalbox.css</stylesheet></action>
        </reference>
    </amazonpaymentindia_adminhtml_logger_api_view>
</layout>
